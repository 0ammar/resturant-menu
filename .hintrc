{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/forms": [
      "default",
      {
        "select-name": "off"
      }
    ],
    "typescript-config/consistent-casing": "off",
    "axe/aria": [
      "default",
      {
        "aria-valid-attr-value": "off"
      }
    ],
    "button-type": "off",
    "no-inline-styles": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "user-select",
          "scrollbar-width",
          "scrollbar-width: none"
        ]
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off"
      }
    ]
  }
}